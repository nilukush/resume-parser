      "Bash(python -m pytest:*)",
      "Bash(python3 -m pytest:*)"
    ]
  },
  "model": "opus",
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); current_dir=$(echo \"$input\" | jq -r '.workspace.current_dir'); basename_dir=$(basename \"$current_dir\"); if git -C \"$current_dir\" rev-parse --git-dir >/dev/null 2>&1; then branch=$(git -C \"$current_dir\" branch --show-current 2>/dev/null || echo 'detached'); if git -C \"$current_dir\" diff-index --quiet HEAD -- 2>/dev/null; then git_info=\" git:($branch)\"; else git_info=\" git:($branch) ✗\"; fi; else git_info=\"\"; fi; printf \"\\033[1;32m➜\\033[0m \\033[36m%s\\033[0m%s\" \"$basename_dir\" \"$git_info\""
  },
