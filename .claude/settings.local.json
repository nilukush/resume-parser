      "Bash(PYTHONPATH=/Users/nileshkumar/gh/resume-parser/backend:*)",
      "Bash(python3.13:*)",
      "Bash(PYTHONPATH=.:*)",
      "Bash(/Users/nileshkumar/gh/resume-parser/backend/.venv/bin/pip:*)",
